    "babel-jest": "^29.7.0",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "expo": {
