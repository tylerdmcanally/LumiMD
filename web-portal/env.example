# Web Portal Environment Variables
# Copy this file to .env.local and fill in your values

# App URL (used for invite links)
NEXT_PUBLIC_APP_URL=https://portal.lumimd.app

# Resend API Key (for sending caregiver invitation emails)
RESEND_API_KEY=<RESEND_API_KEY>

# Subscription paywall
# Set to "true" to show subscription banners/prompts
NEXT_PUBLIC_PAYWALL_ENABLED=false
