{
  "name": "functions",
  "version": "1.0.0",
  "description": "LumiMD Cloud Functions",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "clean:assembly": "ts-node scripts/delete-assembly-transcripts.ts",
    "clean:medication-cache": "ts-node scripts/clearMedicationSafetyCache.ts",
    "backfill:canonical-meds": "ts-node scripts/backfillCanonicalMedicationNames.ts",
    "bypass:grant": "ts-node scripts/set-bypass.ts",
    "bypass:revoke": "ts-node scripts/set-bypass.ts --revoke",
    "bypass:list": "ts-node scripts/list-bypass-users.ts"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "@sentry/node": "^10.29.0",
    "@stablelib/base64": "^1.0.1",
    "axios": "^1.13.2",
    "body-parser": "~1.20.4",
    "chrono-node": "^2.9.0",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.3",
    "helmet": "^8.1.0",
    "openai": "^6.9.1",
    "pdfmake": "^0.2.20",
    "qs": "~6.14.1",
    "resend": "^6.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/pdfmake": "^0.2.12",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
