{
  "name": "functions",
  "version": "1.0.0",
  "description": "LumiMD Cloud Functions",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "clean:assembly": "ts-node scripts/delete-assembly-transcripts.ts",
    "clean:medication-cache": "ts-node scripts/clearMedicationSafetyCache.ts",
    "backfill:canonical-meds": "ts-node scripts/backfillCanonicalMedicationNames.ts"
  },
  "engines": {
    "node": "20"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chrono-node": "^2.9.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.0.1",
    "helmet": "^8.1.0",
    "openai": "^6.9.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
