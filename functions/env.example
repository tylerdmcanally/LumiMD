# Firebase Functions Environment Variables
# Copy this file to .env and fill in your values

# AI Services (required for visit processing)
OPENAI_API_KEY=<OPENAI_API_KEY>
ASSEMBLYAI_API_KEY=<ASSEMBLYAI_API_KEY>

# Firebase Storage
STORAGE_BUCKET=lumimd-dev.appspot.com

# CORS Configuration (comma-separated origins)
ALLOWED_ORIGINS=https://portal.lumimd.app,https://lumimd.app

# Subscription Enforcement
# Set to "true" to disable subscription checks during development
SUBSCRIPTION_ENFORCEMENT_DISABLED=false
