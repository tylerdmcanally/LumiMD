# Environment variables for the Expo client.
# Copy this file to `.env` and fill in secrets for your setup.

# --- Core API configuration ---
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_APP_VERSION=1.0.0

# --- External services ---
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Optional insurance integrations (fill in when enabled)
EXPO_PUBLIC_PVERIFY_API_KEY=your_pverify_api_key
EXPO_PUBLIC_ELIGIBLE_API_KEY=your_eligible_api_key

# --- Recording & storage ---
EXPO_PUBLIC_AUDIO_STORAGE_BUCKET=your_audio_storage_bucket
EXPO_PUBLIC_MAX_RECORDING_DURATION=7200

# --- Compliance / security ---
EXPO_PUBLIC_HIPAA_AUDIT_ENDPOINT=https://your-audit-service.com
EXPO_PUBLIC_DATA_RETENTION_DAYS=2555
EXPO_PUBLIC_ENCRYPTION_KEY=your_32_character_encryption_key_here
